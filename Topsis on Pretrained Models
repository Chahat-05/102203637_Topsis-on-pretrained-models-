   "outputs": [],
   "source": [
    "def topsis(input, weights, impacts):\n",
    "    df=input.iloc[:,1:]\n",
    "\n",
    "    df = normalize(df)\n",
    "    df = weight_normalized(df,weights)\n",
    "\n",
    "    (best,worst) = best_worst(df,impacts)\n",
    "    calc_performance(df,best,worst)\n",
    "    rank(df)\n",
    "    return df"
   ]
  },
